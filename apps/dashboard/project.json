{
  "name": "dashboard",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/dashboard",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{workspaceRoot}/dist/apps/dashboard"],
      "defaultConfiguration": "production",
      "options": {
        "configuration": "Debug",
        "properties": {
          "OutputPath": "apps/dashboard"
        }
      },  
      "configurations": {
        "development": {
          "outputPath": "apps/dashboard",
          "dev": true,
          "optimization": false,
          "sourceMap": true,
          "extractCss": true
        },
        "production": {
          "outputPath": "apps/dashboard",
          "optimization": true,
          "sourceMap": true,
          "extractCss": true
        }
      }
    }
  }
}


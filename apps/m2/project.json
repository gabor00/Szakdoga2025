{
    "name": "m2",
    "targets": {
      "docker-build": {
        "executor": "@nx-tools/nx-container:build",
        "options": {
          "dockerFile": "Dockerfile",
          "context": "apps/m2",
          "push": false,
          "tags": ["m2"]
        }
      },
      "docker-run": {
        "executor": "@nx-tools/nx-container:run",
        "options": {
          "image": "m2",
          "ports": ["8000:8000"]
        }
      }
    }
    }